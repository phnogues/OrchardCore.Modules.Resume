@model IEnumerable<dynamic>

<zone Name="Title">
    <h1>@RenderTitleSegments(T["Resume List"])</h1>
</zone>

@if (Model.Any())
{
    <ul class="list-group">
        @foreach (var contentItemSummary in Model)
        {
            <li class="list-group-item">
                @await DisplayAsync(contentItemSummary)
            </li>
        }
    </ul>
}
else
{
    <div class="alert alert-info">
        @T["No result found."]
    </div>
}
